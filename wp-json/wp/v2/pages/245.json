    <style>
      .agenda__title-container {
        width: 100%;
        height: 80px;
        background-color: rgb(27, 77, 161);
        border-radius: 100px;
        display: flex;
        align-items: center;
        justify-content: center;
        margin-bottom: 30px;
      }

      .agenda__title {
        font-size: 44px;
        color: white;
        text-align: center;
        margin: 0;
        display: inline-block;
      }

      .agenda__swiper {
        overflow: hidden;
        position: relative;
        margin: 0 25px;
      }

      .agenda__month-slider {
        display: flex;
        flex-direction: row;
        flex-wrap: nowrap;
        padding: 20px 0;
        margin-bottom: 0;
      }

      .agenda__month-item {
        list-style: none;
        display: flex;
        width: 150px;
        height: 50px;
        border-radius: 12px;
        margin: 0;
        margin-left: 0 !important;
        background-color: #BCBDBF;
        justify-content: center;
        align-items: center;
        position: relative;
        cursor: pointer;
        transition: background-color ease-in-out 200ms, filter ease-in-out 200ms;
        flex-shrink: 0;
        box-shadow: 0px 4px 4px 0px #00000040;

      }


      .agenda__month-item:hover {
        background-color: #ff6e49;
        filter: grayscale(30%);
      }

      .agenda__month-item:hover .agenda__shape-top,
      .agenda__month-item:hover .agenda__shape-bottom {
        background-color: #ff6e49;
      }

      .agenda__month-item.active,
      .agenda__month-item.active .agenda__shape-top,
      .agenda__month-item.active .agenda__shape-bottom {
        background-color: #FF6E49;
        filter: none;
      }

      .agenda__month-name {
        display: inline-block;
        margin: 0;
        font-size: 20px;
        text-transform: uppercase;
        color: white;
        font-weight: 600;
        pointer-events: none;
      }

      .agenda__shape-top {
        width: 16px;
        height: 15px;
        border-radius: 10px 10px 0 0;
        background-color: #BCBDBF;
        position: absolute;
        top: -10px;
        transition: background-color ease-in-out 200ms, filter ease-in-out 200ms;
        left: 20px;
      }

      .agenda__shape-bottom {
        width: 16px;
        height: 15px;
        border-radius: 0 0 10px 10px;
        background-color: #BCBDBF;
        position: absolute;
        bottom: -10px;
        transition: background-color ease-in-out 200ms, filter ease-in-out 200ms;
        right: 20px;
      }

      .agenda__month-container {
        display: flex;
        flex-direction: column;
        justify-content: flex-start;
        vertical-align: top;
        list-style: none;
        gap: 30px;
      }

      .agenda__month-container.hidden {
        display: none;
      }


      .agenda__course-card {
        display: flex;
        width: 100%;
        gap: 35px;
        margin-left: 0 !important;
        box-shadow: 0px 0px 22.299999237060547px 0px #00000038;
        border-radius: 20px;
        padding: 25px;
      }

      .agenda__course-img-container {
        display: block;
        width: 25%;
        flex-shrink: 0;
        position: relative;
      }

      .agenda__course-img-container img {
        width: 100%;
        height: auto;
        aspect-ratio: 1 / 1;
      }

      .agenda__btns {
        width: 280px;
        flex-shrink: 0;
        display: flex;
        flex-direction: column-reverse;
        justify-content: space-between;
        align-items: flex-end;
      }

      .agenda__titles {
        width: auto;
        flex-grow: 1;
        align-self: center;
        display: flex;
        align-items: flex-start;
        flex-direction: column;
        justify-content: center;
      }

      .agenda__course-title {
        font-size: 35px;
        line-height: 1;
      }

      .agenda__init-date {
        display: block;
        background-color: #70C1BA;
        border-radius: 15px;
        color: white;
        font-size: 22px;
        line-height: 1.1;
        font-weight: 600;
        padding: 8px 18px;
        cursor: pointer;
        text-wrap: nowrap;
        max-width: 100%;
        text-overflow: ellipsis;
        overflow: hidden;
      }

      .agenda__init-date:hover {
        color: white;
      }

      .agenda__cta {
        display: block;
        background-color: rgb(27, 77, 161);
        color: white;
        border-radius: 99px;
        padding: 6px 30px;
        font-weight: 600;
        font-size: 25px;
        transition: transform 150ms linear;
      }

      .agenda__cta>svg {
        position: relative;
        top: 2px;
        width: 18px;
      }

      .agenda__cta:hover {
        color: white;
        transform: scale(1.03);
      }

      .agenda__disertante {
        color: #1B4DA2;
        font-size: 23px;
        font-weight: 700;
        margin-bottom: 10px;
        display: inline-flex;
        align-items: center;
        gap: 8px;
      }

      .agenda__horarios {
        margin-bottom: 10px;
        font-size: 23px;
        display: inline-flex;
        align-items: center;
        gap: 8px;
      }

      .agenda__horarios svg,
      .agenda__disertante svg {
        flex-shrink: 0;
      }

      .agenda__courses-container {
        max-width: 1100px;
        margin: 0 auto;
      }

      .agenda__swiper-container {
        position: relative;
        margin-bottom: 20px;
      }

      .swiper-button-prev,
      .swiper-button-next {
        display: block;
        width: 14px;
        height: 26px;
        position: absolute;
        top: 50%;
        transform: translateY(-50%);
        cursor: pointer;
        transition: opacity 300ms ease-in-out, color 200ms ease-in-out;
        color: #BCBDBF;
      }

      .swiper-button-prev:hover,
      .swiper-button-next:hover {
        color: #FF6E49;
      }

      .swiper-button-prev.swiper-button-disabled,
      .swiper-button-next.swiper-button-disabled {
        opacity: 0;
        pointer-events: none;
      }

      .swiper-button-prev {
        left: 0;
      }

      .swiper-button-next {
        right: 0;
      }

      @media only screen and (max-width: 920px) {
        .agenda__course-img-container {
          width: 20%;
        }

        .agenda__course-title {
          font-size: 28px;
