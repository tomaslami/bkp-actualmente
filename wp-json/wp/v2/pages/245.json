        }

        .agenda__horarios svg,
        .agenda__disertante svg {
          width: 18px;
        }

        .agenda__month-item {
          width: 90px;
          height: 35px;
        }

        .agenda__month-name {
          font-size: 14px;
        }

        .agenda__month-container {
          margin: 0 25px;
        }

      }
    </style>

    <script>
      document.addEventListener("DOMContentLoaded", function () {
        const monthBtns = document.querySelectorAll(".agenda__month-item");
        if (monthBtns) {
          monthBtns.forEach(btn => {
            btn.addEventListener("click", function (e) {

              console.log("clicked", e)
              //first hide everything else
              const months = document.querySelectorAll(".agenda__month-container");
              if (months) {
                months.forEach(month => {
                  month.classList.add("hidden");
                })
              }
              // get all buttons back to normal
              monthBtns.forEach(month_btn => {
                month_btn.classList.remove("active");
              })

              //set this button as selected
              const month_name = e.target.getAttribute("data-month");

              const active_btn = document.querySelector(`.agenda__month-item[data-month="${month_name}"]`)
              active_btn.classList.add("active");

              // then find clicked and show it
              const selected_month = document.querySelector(`.agenda__month-container[data-month="${month_name}"]`);
              if (selected_month) {
                selected_month.classList.remove("hidden");
              }
            })
          })
        }
      });
    </script>

